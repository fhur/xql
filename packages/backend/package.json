{
    "name": "@synthql/backend",
    "type": "module",
    "version": "0.77.4",
    "main": "build/src/index.cjs",
    "module": "build/src/index.js",
    "types": "build/types/src/index.d.ts",
    "sideEffects": false,
    "repository": {
        "type": "git",
        "url": "git@github.com:synthql/SynthQL.git"
    },
    "files": [
        "build",
        "README.md"
    ],
    "license": "MIT",
    "scripts": {
        "link:pkg": "yarn link",
        "test": "vitest",
        "test:ci": "CI=1 vitest run --globals",
        "build": "vite build --mode production",
        "t": "vitest --ui",
        "typecheck": "tsc",
        "checks": "yarn vitest run && yarn tsc",
        "publish:minor": "yarn publish --access public --no-git-tag-version",
        "format": "yarn prettier --config ../../prettier.config.js --write ./src/",
        "benchmarks": "yarn vitest run src/tests/benchmarks/bench.test.ts"
    },
    "dependencies": {
        "@synthql/queries": "0.77.4",
        "kysely": "^0.27.3",
        "pg": "^8.11.3",
        "sql-formatter": "^15.0.2"
    },
    "devDependencies": {
        "@fast-check/vitest": "^0.1.1",
        "@types/benchmark": "^2.1.4",
        "@types/express": "^4.17.21",
        "@types/node": "^20.10.5",
        "@types/pg": "^8.10.9",
        "@vitest/coverage-v8": "^1.2.2",
        "@vitest/ui": "^1.3.1",
        "benchmark": "^2.1.4",
        "dotenv": "^16.3.1",
        "fast-check": "^3.19.0",
        "kysely-codegen": "^0.11.0",
        "next": "^14.2.5",
        "postgres": "^3.4.3",
        "rollup-plugin-node-externals": "^7.1.1",
        "typescript": "^5.4.5",
        "vitest": "^1.1.0"
    }
