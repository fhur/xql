{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "properties": {
        "$schema": {
            "type": "string",
            "description": "Specifies which draft of the JSON Schema standard\nthe schema adheres to",
            "nullable": true
        },
        "out": {
            "type": "string",
            "description": "The output directory for the generated files\ne.g. `src/generated`",
            "nullable": true
        },
        "defaultSchema": {
            "type": "string",
            "description": "The default schema to use e.g. `public`\nThis is similar to the `search_path` in PostgreSQL",
            "nullable": true
        },
        "schemas": {
            "type": "array",
            "description": "The schemas to include in generation\ne.g. `[\"public\", \"company_name\"]`",
            "items": {
                "type": "string"
            },
            "nullable": true
        },
        "tables": {
            "type": "array",
            "description": "The tables to include in generation\ne.g. `[\"users\", \"accounts\"]`",
            "items": {
                "type": "string"
            },
            "nullable": true
        },
        "schemaDefOverrides": {
            "$schema": "http://json-schema.org/draft-07/schema",
            "description": "The table definitions for any tables you want\nto be overriden during the schema generation.\ne.g.:\n\"schemaDefOverrides\": {\n            \"public.actor\": {\n                \"name\": {\n                    \"includable\": { \"type\": \"boolean\", \"const\": false }\n                },\n            },\n            \"accounts.customer\": {\n                \"quantity\": {\n                    \"type\": {\n                        \"type\": \"object\",\n                        \"id\": \"QuantityUnit\",\n                        \"properties\": {\n                            \"unit\": {\n                                \"type\": \"string\",\n                                \"description\": \"A unit string, property in JSONB object\"\n                            }\n                        },\n                        \"description\": \"A QuantityUnit object, casted from a JSONB column\"\n                    }\n                },\n            },\n        }",
            "type": "object",
            "patternProperties": {
                "^[^.]+.[^.]+$": {
                    "type": "object",
                    "description": "The column definitions for each table to be overriden.\nThe key for each table should be the qualified name for the table,\nin the form `${schemaName}.${tableName}` e.g `public.users`",
                    "patternProperties": {
                        "^[^.]+$": {
                            "description": "The attribute definitions for each column to be overriden.\nThe key for each column should be the name of the column,\nin the form `${columnName}` e.g `first_name`",
                            "type": "object",
                            "properties": {
                                "type": {
                                    "type": "object",
                                    "properties": {
                                        "id": {
                                            "type": "string"
                                        },
                                        "description": {
                                            "type": "string"
                                        },
                                        "type": {
                                            "type": "string"
                                        },
                                        "title": {
                                            "type": "string",
                                            "nullable": true
                                        },
                                        "tsType": {
                                            "type": "string",
                                            "nullable": true
                                        },
                                        "minimum": {
                                            "type": "number",
                                            "nullable": true
                                        },
                                        "maximum": {
                                            "type": "number",
                                            "nullable": true
                                        },
                                        "format": {
                                            "type": "string",
                                            "nullable": true
                                        },
                                        "enum": {
                                            "type": "array",
                                            "items": {
                                                "type": "string"
                                            },
                                            "nullable": true
                                        },
                                        "properties": {
                                            "type": "object",
                                            "patternProperties": {
                                                "^[^.]+$": {
                                                    "description": "The property definitions for each object property to be overriden.\nThe key for each property should be the name of the property,\nin the form `${propertyName}` e.g `unit`",
                                                    "type": "object",
                                                    "properties": {
                                                        "type": {
                                                            "type": "string"
                                                        },
                                                        "id": {
                                                            "type": "string",
                                                            "nullable": true
                                                        },
                                                        "description": {
                                                            "type": "string",
                                                            "nullable": true
                                                        },
                                                        "title": {
                                                            "type": "string",
                                                            "nullable": true
                                                        },
                                                        "tsType": {
                                                            "type": "string",
                                                            "nullable": true
                                                        },
                                                        "minimum": {
                                                            "type": "number",
                                                            "nullable": true
                                                        },
                                                        "maximum": {
                                                            "type": "number",
                                                            "nullable": true
                                                        },
                                                        "format": {
                                                            "type": "string",
                                                            "nullable": true
                                                        },
                                                        "enum": {
                                                            "type": "array",
                                                            "items": {
                                                                "type": "string"
                                                            },
                                                            "nullable": true
                                                        }
                                                    },
                                                    "required": ["type"],
                                                    "additionalProperties": true
                                                }
                                            },
                                            "nullable": true,
                                            "required": [],
                                            "additionalProperties": true
                                        }
                                    },
                                    "nullable": true,
                                    "required": ["id", "type", "description"],
                                    "additionalProperties": true
                                },
                                "selectable": {
                                    "type": "object",
                                    "properties": {
                                        "type": {
                                            "type": "string"
                                        },
                                        "const": {
                                            "type": "boolean"
                                        }
                                    },
                                    "nullable": true,
                                    "required": ["type", "const"],
                                    "additionalProperties": true
                                },
                                "includable": {
                                    "type": "object",
                                    "properties": {
                                        "type": {
                                            "type": "string"
                                        },
                                        "const": {
                                            "type": "boolean"
                                        }
                                    },
                                    "nullable": true,
                                    "required": ["type", "const"],
                                    "additionalProperties": true
                                },
                                "whereable": {
                                    "type": "object",
                                    "properties": {
                                        "type": {
                                            "type": "string"
                                        },
                                        "const": {
                                            "type": "boolean"
                                        }
                                    },
                                    "nullable": true,
                                    "required": ["type", "const"],
                                    "additionalProperties": true
                                },
                                "nullable": {
                                    "type": "object",
                                    "properties": {
                                        "type": {
                                            "type": "string"
                                        },
                                        "const": {
                                            "type": "boolean"
                                        }
                                    },
                                    "nullable": true,
                                    "required": ["type", "const"],
                                    "additionalProperties": true
                                },
                                "isPrimaryKey": {
                                    "type": "object",
                                    "properties": {
                                        "type": {
                                            "type": "string"
                                        },
                                        "const": {
                                            "type": "boolean"
                                        }
                                    },
                                    "nullable": true,
                                    "required": ["type", "const"],
                                    "additionalProperties": true
                                }
                            },
                            "required": [],
                            "additionalProperties": true
                        }
                    },
                    "required": [],
                    "additionalProperties": true
                }
            },
            "required": [],
            "additionalProperties": true,
            "nullable": true
        }
    },
    "required": [],
    "additionalProperties": true
}
